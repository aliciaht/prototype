<h1>Listing brandlists</h1>

<table>
  <tr>
    <th>Company</th>
    <th>Brand</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @brandlists.each do |brandlist| %>
  <tr>
    <td><%= brandlist.company %></td>
    <td><%= brandlist.brand %></td>
    <td><%= link_to 'Show', brandlist %></td>
    <td><%= link_to 'Edit', edit_brandlist_path(brandlist) %></td>
    <td><%= link_to 'Destroy', brandlist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Brandlist', new_brandlist_path %>
